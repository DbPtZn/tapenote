    "class-validator": "^0.14.1",
    "cos-nodejs-sdk-v5": "^2.14.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express-http-proxy": "^2.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "install": "^0.13.0",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",

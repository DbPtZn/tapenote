{
  "name": "@tapenote/server",
  "version": "0.0.1",
  "description": "tapenote server",
  "author": "boxin du",
  "license": "GPL-3.0-only",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/main",
    "build": "nest build",
    "build:electron": "cross-env NODE_ENV=electron nest build --webpack --webpackPath=./webpack.config.js",
    "webpack": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "dev:electron": "cross-env NODE_ENV=electron nest start --watch",
    "dev:prod": "cross-env NODE_ENV=production nest start --watch",
    "start:dev": "cross-env NODE_ENV=development node dist/main",
    "start:debug": "nest start --debug --watch",
    "start:electron": "cross-env NODE_ENV=electron node process/server.cjs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@alicloud/nls-filetrans-2018-08-17": "^1.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@nestjs/axios": "^3.0.3",
